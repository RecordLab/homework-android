<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.profile.account.ProfileAccountActivity">

    <View
        android:id="@+id/view_lock_setting_toggle"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" ></View>

    <TextView
        android:id="@+id/tv_lock_setting_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="이메일 주소"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_lock_setting_toggle"
        app:layout_constraintStart_toStartOf="@+id/view_lock_setting_toggle"
        app:layout_constraintTop_toTopOf="@+id/view_lock_setting_toggle" />

    <View
        android:id="@+id/bg45"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_lock_setting_toggle"></View>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="닉네임"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/bg45"
        app:layout_constraintStart_toStartOf="@+id/bg45"
        app:layout_constraintTop_toTopOf="@+id/bg45" />

    <View
        android:id="@+id/bg454"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bg45"></View>

    <TextView
        android:id="@+id/tvChangePassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="비밀번호 변경"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/bg454"
        app:layout_constraintStart_toStartOf="@+id/bg454"
        app:layout_constraintTop_toTopOf="@+id/bg454" />

    <View
        android:id="@+id/bg4544"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bg454"></View>

    <TextView
        android:id="@+id/textView1454"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="회원 탈퇴 / 연동 끊기"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/bg4544"
        app:layout_constraintStart_toStartOf="@+id/bg4544"
        app:layout_constraintTop_toTopOf="@+id/bg4544" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="ssafy@ssafy.com"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_lock_setting_toggle"
        app:layout_constraintStart_toEndOf="@+id/tv_lock_setting_title"
        app:layout_constraintTop_toTopOf="@+id/view_lock_setting_toggle" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="ssafy205"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/bg45"
        app:layout_constraintStart_toEndOf="@+id/tv_lock_setting_title"
        app:layout_constraintTop_toBottomOf="@+id/view_lock_setting_toggle" />

    <include
        android:id="@+id/include"
        layout="@layout/item_custom_actionbar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/view_lock_setting_toggle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:fontFamily="@font/spoqa_han_sans_neo_medium"
        android:text="계정"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/view_lock_setting_toggle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include" />

