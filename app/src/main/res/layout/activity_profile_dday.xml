<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.profile.day.ProfileDdayActivity">

    <View
        android:id="@+id/view_lock_setting_toggle"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" ></View>

    <TextView
        android:id="@+id/tv_lock_setting_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="디데이 설정"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_lock_setting_toggle"
        app:layout_constraintStart_toStartOf="@+id/view_lock_setting_toggle"
        app:layout_constraintTop_toTopOf="@+id/view_lock_setting_toggle" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_lock_setting_toggle"
        app:layout_constraintEnd_toEndOf="@+id/view_lock_setting_toggle"
        app:layout_constraintTop_toTopOf="@+id/view_lock_setting_toggle"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck" />

    <View
        android:id="@+id/bg45"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="16dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_lock_setting_toggle"></View>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="디데이"
        android:textColor="@color/disableGray"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/bg45"
        app:layout_constraintStart_toStartOf="@+id/bg45"
        app:layout_constraintTop_toTopOf="@+id/bg45" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="21.10.29"
        android:textColor="@color/disableGray"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/bg45"
        app:layout_constraintEnd_toEndOf="@+id/bg45"
        app:layout_constraintTop_toBottomOf="@+id/view_lock_setting_toggle" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toTopOf="@+id/view_lock_setting_toggle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_ios_black_24" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:fontFamily="@font/spoqa_han_sans_neo_medium"
        android:text="디데이"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/view_lock_setting_toggle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>